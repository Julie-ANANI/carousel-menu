##!/bin/bash

#$APP_NAME --environment=$BUILD_ENV $OTHER_PARAMS
LATEST=latest
VENDOR=vendor
FRONT_VERSION=umi
BUILD_ENV=prod

if [ $CACHE_TAG = $VENDOR ]
then
  FRONT_VERSION=$VENDOR
fi
echo Building image using $FRONT_VERSION

if [ $CACHE_TAG = $LATEST ]
then
  BUILD_ENV=dev
fi
echo $BUILD_ENV

docker build --build-arg APP_NAME=$FRONT_VERSION BUILD_ENV=$BUILD_ENV -t $IMAGE_NAME
