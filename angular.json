  "defaultProject": "umi",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app",
      "style": "scss"
    }
  },
  "cli": {
    "analytics": false
  }
}
