{
  "name": "umi-application-client",
  "version": "2.10.18-RC4",
  "license": "MIT",
  "scripts": {
    "serve": "ng serve umi -c=local",
    "serve:dynergie": "ng serve dynergie -c=local",
    "serve:multivalente": "ng serve multivalente -c=local",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build umi -c=local",
    "build:prod-dynergie": "ng build dynergie -c=local",
    "build:prod-multivalente": "ng build multivalente -c=local",
    "build:universal": "npm run build:ssr && node dist/server/main.js",
    "build:webpack": "npm run build:ssr && gzip -k -r dist/browser/ && npm run server:webpack && npm run serve:ssr",
    "build:ssr": "npm run build:prod && npm run build:server",
    "build:ssr-dynergie": "npm run build:prod-dynergie && npm run build:server-dynergie",
    "build:ssr-multivalente": "npm run build:prod-multivalente && npm run build:server-multivalente",
    "build:server": "ng run umi:server -c=local",
    "build:server-dynergie": "ng run dynergie:server -c=local",
    "build:server-multivalente": "ng run multivalente:server -c=local",
    "serve:ssr": "node dist/server",
    "server:webpack": "webpack --config webpack.server.config.js --progress --color",
    "bundle-report": "ng build umi -c=production --stats-json && webpack-bundle-analyzer dist/browser/stats-es2015.json"
  },
  "private": true,
