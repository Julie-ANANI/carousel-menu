{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "umi-application-client"
  },
  "apps": [
    {
      "name": "umi",
      "root": "src",
      "outDir": "./dist",
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../node_modules/font-awesome/css/font-awesome.css"
      ],
      "assets": [
        {
          "glob": "**/*",
          "input": "../node_modules/tinymce/skins",
          "output": "./assets/skins"
        }
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/hammerjs/hammer.js",
        "../node_modules/chart.js/dist/Chart.min.js",
        "../node_modules/tinymce/tinymce.min.js",
        "../node_modules/tinymce/themes/modern/theme.min.js",
        "../node_modules/tinymce/plugins/link/plugin.min.js",
        "../node_modules/tinymce/plugins/paste/plugin.min.js",
        "../node_modules/tinymce/plugins/table/plugin.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "local": "environments/umi/environment.local.ts",
        "dev": "environments/umi/environment.dev.ts",
        "prod": "environments/umi/environment.prod.ts"
      }
    },
    {
      "name": "vendor1",
      "root": "src",
      "outDir": "./dist",
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.vendor1.scss"
      ],
      "assets": [
        {
          "glob": "**/*",
          "input": "../node_modules/tinymce/skins",
          "output": "./assets/skins"
        }
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/hammerjs/hammer.js",
        "https://canvasjs.com/assets/script/canvasjs.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "vendor1.local": "environments/vendor1/environment.vendor1.local.ts",
        "vendor1.dev": "environments/vendor1/environment.vendor1.dev.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
